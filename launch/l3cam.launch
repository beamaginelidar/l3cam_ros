<launch>
    <!-- roslaunch l3cam_ros l3cam.launch stream:=true configure:=true rviz:=false rqt_reconfigure:=false -->
    <arg name="stream" default="true" />
    <arg name="rviz" default="false" />
    <arg name="configure" default="true" />
    <arg name="rqt_reconfigure" default="false" />

    <!-- Network configuration -->
    <arg name="ip_address" default="192.168.1.250" />
    <arg name="netmask" default="255.255.255.0" />
    <arg name="gateway" default="0.0.0.0" />
    <arg name="dhcp" default="false" />

    <!-- Pointcloud configuration -->
    <arg name="pointcloud_color" default="0" /> <!-- see pointCloudColor -->
    <arg name="pointcloud_color_range_minimum" default="0" /> <!-- 0 - 400000 -->
    <arg name="pointcloud_color_range_maximum" default="400000" /> <!-- 0 - 400000 -->
    <arg name="distance_range_minimum" default="0" /> <!-- 0 - 400000 -->
    <arg name="distance_range_maximum" default="400000" /> <!-- 0 - 400000 -->

    <!-- Polarimetric configuration -->
    <arg name="polarimetric_camera_brightness" default="127" /> <!-- 0 - 255 -->
    <arg name="polarimetric_camera_black_level" default="6.0" /> <!-- 0 - 12.5 -->
    <arg name="polarimetric_camera_auto_gain" default="true" />
    <arg name="polarimetric_camera_auto_gain_range_minimum" default="0.0" /> <!-- 0 - 48 -->
    <arg name="polarimetric_camera_auto_gain_range_maximum" default="48.0" /> <!-- 0 - 48 -->
    <arg name="polarimetric_camera_gain" default="24.0" /> <!-- 0 - 48 -->
    <arg name="polarimetric_camera_auto_exposure_time" default="true" />
    <arg name="polarimetric_camera_auto_exposure_time_range_minimum" default="33.456" /> <!-- 33.456 - 1000000 -->
    <arg name="polarimetric_camera_auto_exposure_time_range_maximum" default="1000000.0" /> <!-- 33.456 - 1000000 -->
    <arg name="polarimetric_camera_exposure_time" default="500000.0" /> <!-- 33.456 - 1000000 -->

    <!-- RGB configuration -->
    <arg name="rgb_camera_brightness" default="0" /> <!-- -15 - 15 -->
    <arg name="rgb_camera_contrast" default="10" /> <!-- 0 - 30 -->
    <arg name="rgb_camera_saturation" default="16" /> <!-- 0 - 60 -->
    <arg name="rgb_camera_sharpness" default="16" /> <!-- 0 - 127 -->
    <arg name="rgb_camera_gamma" default="220" /> <!-- 40 - 500 -->
    <arg name="rgb_camera_gain" default="0" /> <!-- 0 - 63 -->
    <arg name="rgb_camera_auto_white_balance" default="true" />
    <arg name="rgb_camera_white_balance" default="5000" /> <!-- 1000 - 10000 -->
    <arg name="rgb_camera_auto_exposure_time" default="true" />
    <arg name="rgb_camera_exposure_time" default="156" /> <!-- 1 - 10000 -->
    <arg name="rgb_camera_resolution" default="3" /> <!-- see econResolutions -->
    <arg name="rgb_camera_framerate" default="10" /> <!-- 1 - 16 -->

    <!-- Thermal configuration -->
    <arg name="thermal_camera_colormap" default="1" /> <!-- see thermalTypes -->
    <arg name="thermal_camera_temperature_filter" default="false" />
    <arg name="thermal_camera_temperature_filter_min" default="0" /> <!-- -40 - 200 -->
    <arg name="thermal_camera_temperature_filter_max" default="50" /> <!-- -40 - 200 -->

    <!-- Allied Wide configuration -->
    <arg name="allied_wide_camera_black_level" default="0" /> <!-- 0 - 4095 -->
    <arg name="allied_wide_camera_exposure_time" default="4992.32" /> <!-- 63 - 10000000 -->
    <arg name="allied_wide_camera_auto_exposure_time" default="false" />
    <arg name="allied_wide_camera_auto_exposure_time_range_min" default="87.596" /> <!-- 63.03 - 8999990 -->
    <arg name="allied_wide_camera_auto_exposure_time_range_max" default="87.596" /> <!-- 87.596 - 10000000 -->
    <arg name="allied_wide_camera_gain" default="0" /> <!-- 0 - 48 -->
    <arg name="allied_wide_camera_auto_gain" default="false" />
    <arg name="allied_wide_camera_auto_gain_range_min" default="0" /> <!-- 0 - 48 -->
    <arg name="allied_wide_camera_auto_gain_range_max" default="48" /> <!-- 0 - 48 -->
    <arg name="allied_wide_camera_gamma" default="1" /> <!-- 0.4 - 2.4 -->
    <arg name="allied_wide_camera_saturation" default="1" /> <!-- 0 - 2 -->
    <arg name="allied_wide_camera_sharpness" default="0" /> <!-- -12 - 12 -->
    <arg name="allied_wide_camera_hue" default="0" /> <!-- -40 - 40 -->
    <arg name="allied_wide_camera_intensity_auto_precedence" default="0" /> <!-- 0(MinimizeNoise) or 1(MinimizeBlur) -->
    <arg name="allied_wide_camera_auto_white_balance" default="false" />
    <arg name="allied_wide_camera_balance_ratio_selector" default="0" /> <!-- 0(Red), 1(Blue) -->
    <arg name="allied_wide_camera_balance_ratio" default="2.35498" /> <!-- 0 - 8 -->
    <arg name="allied_wide_camera_balance_white_auto_rate" default="100" /> <!-- 0 - 100 -->
    <arg name="allied_wide_camera_balance_white_auto_tolerance" default="5" /> <!-- 0 - 50 -->
    <arg name="allied_wide_camera_auto_mode_region_height" default="1028" /> <!-- 0 - 1028 -->
    <arg name="allied_wide_camera_auto_mode_region_width" default="1232" /> <!-- 0 - 1232 -->
    <arg name="allied_wide_camera_intensity_controller_region" default="0" /> <!-- 0(AutoMode), 4(FullImage) -->
    <arg name="allied_wide_camera_intensity_controller_target" default="50" /> <!-- 10 - 90 -->
    <arg name="allied_wide_camera_max_driver_buffers_count" default="64" /> <!-- 1 - 4096 -->

    <!-- Allied Narrow configuration -->
    <arg name="allied_narrow_camera_black_level" default="0" /> <!-- 0 - 4095 -->
    <arg name="allied_narrow_camera_exposure_time" default="4992.32" /> <!-- 63 - 10000000 -->
    <arg name="allied_narrow_camera_auto_exposure_time" default="false" />
    <arg name="allied_narrow_camera_auto_exposure_time_range_min" default="87.596" /> <!-- 63.03 - 8999990 -->
    <arg name="allied_narrow_camera_auto_exposure_time_range_max" default="87.596" /> <!-- 87.596 - 10000000 -->
    <arg name="allied_narrow_camera_gain" default="0" /> <!-- 0 - 48 -->
    <arg name="allied_narrow_camera_auto_gain" default="false" />
    <arg name="allied_narrow_camera_auto_gain_range_min" default="0" /> <!-- 0 - 48 -->
    <arg name="allied_narrow_camera_auto_gain_range_max" default="48" /> <!-- 0 - 48 -->
    <arg name="allied_narrow_camera_gamma" default="1" /> <!-- 0.4 - 2.4 -->
    <arg name="allied_narrow_camera_saturation" default="1" /> <!-- 0 - 2 -->
    <arg name="allied_narrow_camera_sharpness" default="0" /> <!-- -12 - 12 -->
    <arg name="allied_narrow_camera_hue" default="0" /> <!-- -40 - 40 -->
    <arg name="allied_narrow_camera_intensity_auto_precedence" default="0" /> <!-- 0(MinimizeNoise) or 1(MinimizeBlur) -->
    <arg name="allied_narrow_camera_auto_white_balance" default="false" />
    <arg name="allied_narrow_camera_balance_ratio_selector" default="0" /> <!-- 0(Red), 1(Blue) -->
    <arg name="allied_narrow_camera_balance_ratio" default="2.35498" /> <!-- 0 - 8 -->
    <arg name="allied_narrow_camera_balance_white_auto_rate" default="100" /> <!-- 0 - 100 -->
    <arg name="allied_narrow_camera_balance_white_auto_tolerance" default="5" /> <!-- 0 - 50 -->
    <arg name="allied_narrow_camera_auto_mode_region_height" default="2056" /> <!-- 0 - 2056 -->
    <arg name="allied_narrow_camera_auto_mode_region_width" default="2464" /> <!-- 0 - 2464 -->
    <arg name="allied_narrow_camera_intensity_controller_region" default="0" /> <!-- 0(AutoMode), 4(FullImage) -->
    <arg name="allied_narrow_camera_intensity_controller_target" default="50" /> <!-- 10 - 90 -->
    <arg name="allied_narrow_camera_max_driver_buffers_count" default="64" /> <!-- 1 - 4096 -->

    <!-- Start the driver -->
    <node pkg="l3cam_ros" type="l3cam_ros_node" name="l3cam_ros_node" output="screen">
        <param name="pointcloud_color" value="$(arg pointcloud_color)" type="int" />
        <param name="pointcloud_color_range_minimum" value="$(arg pointcloud_color_range_minimum)" type="int" />
        <param name="pointcloud_color_range_maximum" value="$(arg pointcloud_color_range_maximum)" type="int" />
        <param name="distance_range_minimum" value="$(arg distance_range_minimum)" type="int" />
        <param name="distance_range_maximum" value="$(arg distance_range_maximum)" type="int" />

        <param name="polarimetric_camera_brightness" value="$(arg polarimetric_camera_brightness)" type="int" />
        <param name="polarimetric_camera_black_level" value="$(arg polarimetric_camera_black_level)" type="double" />
        <param name="polarimetric_camera_auto_gain" value="$(arg polarimetric_camera_auto_gain)" type="bool" />
        <param name="polarimetric_camera_auto_gain_range_minimum" value="$(arg polarimetric_camera_auto_gain_range_minimum)" type="double" />
        <param name="polarimetric_camera_auto_gain_range_maximum" value="$(arg polarimetric_camera_auto_gain_range_maximum)" type="double" />
        <param name="polarimetric_camera_gain" value="$(arg polarimetric_camera_gain)" type="double" />
        <param name="polarimetric_camera_auto_exposure_time" value="$(arg polarimetric_camera_auto_exposure_time)" type="bool" />
        <param name="polarimetric_camera_auto_exposure_time_range_minimum" value="$(arg polarimetric_camera_auto_exposure_time_range_minimum)" type="double" />
        <param name="polarimetric_camera_auto_exposure_time_range_maximum" value="$(arg polarimetric_camera_auto_exposure_time_range_maximum)" type="double" />
        <param name="polarimetric_camera_exposure_time" value="$(arg polarimetric_camera_exposure_time)" type="double" />

        <param name="rgb_camera_brightness" value="$(arg rgb_camera_brightness)" type="int" />
        <param name="rgb_camera_contrast" value="$(arg rgb_camera_contrast)" type="int" />
        <param name="rgb_camera_saturation" value="$(arg rgb_camera_saturation)" type="int" />
        <param name="rgb_camera_sharpness" value="$(arg rgb_camera_sharpness)" type="int" />
        <param name="rgb_camera_gamma" value="$(arg rgb_camera_gamma)" type="int" />
        <param name="rgb_camera_gain" value="$(arg rgb_camera_gain)" type="int" />
        <param name="rgb_camera_auto_white_balance" value="$(arg rgb_camera_auto_white_balance)" type="bool" />
        <param name="rgb_camera_white_balance" value="$(arg rgb_camera_white_balance)" type="int" />
        <param name="rgb_camera_auto_exposure_time" value="$(arg rgb_camera_auto_exposure_time)" type="bool" />
        <param name="rgb_camera_exposure_time" value="$(arg rgb_camera_exposure_time)" type="int" />
        <param name="rgb_camera_resolution" value="$(arg rgb_camera_resolution)" type="int" />
        <param name="rgb_camera_framerate" value="$(arg rgb_camera_framerate)" type="int" />

        <param name="thermal_camera_colormap" value="$(arg thermal_camera_colormap)" type="int" />
        <param name="thermal_camera_temperature_filter" value="$(arg thermal_camera_temperature_filter)" type="bool" />
        <param name="thermal_camera_temperature_filter_min" value="$(arg thermal_camera_temperature_filter_min)" type="int" />
        <param name="thermal_camera_temperature_filter_max" value="$(arg thermal_camera_temperature_filter_max)" type="int" />
        
        <param name="allied_wide_camera_black_level" value="$(arg allied_wide_camera_black_level)" type="double" />
        <param name="allied_wide_camera_exposure_time" value="$(arg allied_wide_camera_exposure_time)" type="double" />
        <param name="allied_wide_camera_auto_exposure_time" value="$(arg allied_wide_camera_auto_exposure_time)" type="bool" />
        <param name="allied_wide_camera_auto_exposure_time_range_min" value="$(arg allied_wide_camera_auto_exposure_time_range_min)" type="double" />
        <param name="allied_wide_camera_auto_exposure_time_range_max" value="$(arg allied_wide_camera_auto_exposure_time_range_max)" type="double" />
        <param name="allied_wide_camera_gain" value="$(arg allied_wide_camera_gain)" type="double" />
        <param name="allied_wide_camera_auto_gain" value="$(arg allied_wide_camera_auto_gain)" type="bool" />
        <param name="allied_wide_camera_auto_gain_range_min" value="$(arg allied_wide_camera_auto_gain_range_min)" type="double" />
        <param name="allied_wide_camera_auto_gain_range_max" value="$(arg allied_wide_camera_auto_gain_range_max)" type="double" />
        <param name="allied_wide_camera_gamma" value="$(arg allied_wide_camera_gamma)" type="double" />
        <param name="allied_wide_camera_saturation" value="$(arg allied_wide_camera_saturation)" type="double" />
        <param name="allied_wide_camera_sharpness" value="$(arg allied_wide_camera_sharpness)" type="double" />
        <param name="allied_wide_camera_hue" value="$(arg allied_wide_camera_hue)" type="double" />
        <param name="allied_wide_camera_intensity_auto_precedence" value="$(arg allied_wide_camera_intensity_auto_precedence)" type="int" />
        <param name="allied_wide_camera_auto_white_balance" value="$(arg allied_wide_camera_auto_white_balance)" type="bool" />
        <param name="allied_wide_camera_balance_ratio_selector" value="$(arg allied_wide_camera_balance_ratio_selector)" type="int" />
        <param name="allied_wide_camera_balance_ratio" value="$(arg allied_wide_camera_balance_ratio)" type="double" />
        <param name="allied_wide_camera_balance_white_auto_rate" value="$(arg allied_wide_camera_balance_white_auto_rate)" type="double" />
        <param name="allied_wide_camera_balance_white_auto_tolerance" value="$(arg allied_wide_camera_balance_white_auto_tolerance)" type="double" />
        <param name="allied_wide_camera_auto_mode_region_height" value="$(arg allied_wide_camera_auto_mode_region_height)" type="int" />
        <param name="allied_wide_camera_auto_mode_region_width" value="$(arg allied_wide_camera_auto_mode_region_width)" type="int" />
        <param name="allied_wide_camera_intensity_controller_region" value="$(arg allied_wide_camera_intensity_controller_region)" type="int" />
        <param name="allied_wide_camera_intensity_controller_target" value="$(arg allied_wide_camera_intensity_controller_target)" type="double" />
        <param name="allied_wide_camera_max_driver_buffers_count" value="$(arg allied_wide_camera_max_driver_buffers_count)" type="int" />

        <param name="allied_narrow_camera_black_level" value="$(arg allied_narrow_camera_black_level)" type="double" />
        <param name="allied_narrow_camera_exposure_time" value="$(arg allied_narrow_camera_exposure_time)" type="double" />
        <param name="allied_narrow_camera_auto_exposure_time" value="$(arg allied_narrow_camera_auto_exposure_time)" type="bool" />
        <param name="allied_narrow_camera_auto_exposure_time_range_min" value="$(arg allied_narrow_camera_auto_exposure_time_range_min)" type="double" />
        <param name="allied_narrow_camera_auto_exposure_time_range_max" value="$(arg allied_narrow_camera_auto_exposure_time_range_max)" type="double" />
        <param name="allied_narrow_camera_gain" value="$(arg allied_narrow_camera_gain)" type="double" />
        <param name="allied_narrow_camera_auto_gain" value="$(arg allied_narrow_camera_auto_gain)" type="bool" />
        <param name="allied_narrow_camera_auto_gain_range_min" value="$(arg allied_narrow_camera_auto_gain_range_min)" type="double" />
        <param name="allied_narrow_camera_auto_gain_range_max" value="$(arg allied_narrow_camera_auto_gain_range_max)" type="double" />
        <param name="allied_narrow_camera_gamma" value="$(arg allied_narrow_camera_gamma)" type="double" />
        <param name="allied_narrow_camera_saturation" value="$(arg allied_narrow_camera_saturation)" type="double" />
        <param name="allied_narrow_camera_sharpness" value="$(arg allied_narrow_camera_sharpness)" type="double" />
        <param name="allied_narrow_camera_hue" value="$(arg allied_narrow_camera_hue)" type="double" />
        <param name="allied_narrow_camera_intensity_auto_precedence" value="$(arg allied_narrow_camera_intensity_auto_precedence)" type="int" />
        <param name="allied_narrow_camera_auto_white_balance" value="$(arg allied_narrow_camera_auto_white_balance)"  type="bool" />
        <param name="allied_narrow_camera_balance_ratio_selector" value="$(arg allied_narrow_camera_balance_ratio_selector)" type="int" />
        <param name="allied_narrow_camera_balance_ratio" value="$(arg allied_narrow_camera_balance_ratio)" type="double" />
        <param name="allied_narrow_camera_balance_white_auto_rate" value="$(arg allied_narrow_camera_balance_white_auto_rate)" type="double" />
        <param name="allied_narrow_camera_balance_white_auto_tolerance" value="$(arg allied_narrow_camera_balance_white_auto_tolerance)" type="double" />
        <param name="allied_narrow_camera_auto_mode_region_height" value="$(arg allied_narrow_camera_auto_mode_region_height)" type="int" />
        <param name="allied_narrow_camera_auto_mode_region_width" value="$(arg allied_narrow_camera_auto_mode_region_width)" type="int" />
        <param name="allied_narrow_camera_intensity_controller_region" value="$(arg allied_narrow_camera_intensity_controller_region)" type="int" />
        <param name="allied_narrow_camera_intensity_controller_target" value="$(arg allied_narrow_camera_intensity_controller_target)" type="double" />
        <param name="allied_narrow_camera_max_driver_buffers_count" value="$(arg allied_narrow_camera_max_driver_buffers_count)" type="int" />
    </node>

    <!-- Start dyanmic reconfigure -->
    <include file="$(find l3cam_ros)/launch/configure_l3cam.launch" if="$(eval arg('configure'))">
        <arg name="rqt_reconfigure" value="$(arg rqt_reconfigure)" />

        <arg name="ip_address" value="$(arg ip_address)" />
        <arg name="netmask" value="$(arg netmask)" />
        <arg name="gateway" value="$(arg gateway)" />
        <arg name="dhcp" value="$(arg dhcp)" />

        <arg name="pointcloud_color" value="$(arg pointcloud_color)" />
        <arg name="pointcloud_color_range_minimum" value="$(arg pointcloud_color_range_minimum)" />
        <arg name="pointcloud_color_range_maximum" value="$(arg pointcloud_color_range_maximum)" />
        <arg name="distance_range_minimum" value="$(arg distance_range_minimum)" />
        <arg name="distance_range_maximum" value="$(arg distance_range_maximum)" />

        <arg name="polarimetric_camera_brightness" value="$(arg polarimetric_camera_brightness)" />
        <arg name="polarimetric_camera_black_level" value="$(arg polarimetric_camera_black_level)" />
        <arg name="polarimetric_camera_auto_gain" value="$(arg polarimetric_camera_auto_gain)" />
        <arg name="polarimetric_camera_auto_gain_range_minimum" value="$(arg polarimetric_camera_auto_gain_range_minimum)" />
        <arg name="polarimetric_camera_auto_gain_range_maximum" value="$(arg polarimetric_camera_auto_gain_range_maximum)" />
        <arg name="polarimetric_camera_gain" value="$(arg polarimetric_camera_gain)" />
        <arg name="polarimetric_camera_auto_exposure_time" value="$(arg polarimetric_camera_auto_exposure_time)" />
        <arg name="polarimetric_camera_auto_exposure_time_range_minimum" value="$(arg polarimetric_camera_auto_exposure_time_range_minimum)" />
        <arg name="polarimetric_camera_auto_exposure_time_range_maximum" value="$(arg polarimetric_camera_auto_exposure_time_range_maximum)" />
        <arg name="polarimetric_camera_exposure_time" value="$(arg polarimetric_camera_exposure_time)" />

        <arg name="rgb_camera_brightness" value="$(arg rgb_camera_brightness)" />
        <arg name="rgb_camera_contrast" value="$(arg rgb_camera_contrast)" />
        <arg name="rgb_camera_saturation" value="$(arg rgb_camera_saturation)" />
        <arg name="rgb_camera_sharpness" value="$(arg rgb_camera_sharpness)" />
        <arg name="rgb_camera_gamma" value="$(arg rgb_camera_gamma)" />
        <arg name="rgb_camera_gain" value="$(arg rgb_camera_gain)" />
        <arg name="rgb_camera_auto_white_balance" value="$(arg rgb_camera_auto_white_balance)" />
        <arg name="rgb_camera_white_balance" value="$(arg rgb_camera_white_balance)" />
        <arg name="rgb_camera_auto_exposure_time" value="$(arg rgb_camera_auto_exposure_time)" />
        <arg name="rgb_camera_exposure_time" value="$(arg rgb_camera_exposure_time)" />

        <arg name="thermal_camera_colormap" value="$(arg thermal_camera_colormap)" />
        <arg name="thermal_camera_temperature_filter" value="$(arg thermal_camera_temperature_filter)" />
        <arg name="thermal_camera_temperature_filter_min" value="$(arg thermal_camera_temperature_filter_min)" />
        <arg name="thermal_camera_temperature_filter_max" value="$(arg thermal_camera_temperature_filter_max)" />

        <arg name="allied_wide_camera_exposure_time" value="$(arg allied_wide_camera_exposure_time)" />
        <arg name="allied_wide_camera_auto_exposure_time" value="$(arg allied_wide_camera_auto_exposure_time)" />
        <arg name="allied_wide_camera_auto_exposure_time_range_min" value="$(arg allied_wide_camera_auto_exposure_time_range_min)" />
        <arg name="allied_wide_camera_auto_exposure_time_range_max" value="$(arg allied_wide_camera_auto_exposure_time_range_max)" />
        <arg name="allied_wide_camera_gain" value="$(arg allied_wide_camera_gain)" />
        <arg name="allied_wide_camera_auto_gain" value="$(arg allied_wide_camera_auto_gain)" />
        <arg name="allied_wide_camera_auto_gain_range_min" value="$(arg allied_wide_camera_auto_gain_range_min)" />
        <arg name="allied_wide_camera_auto_gain_range_max" value="$(arg allied_wide_camera_auto_gain_range_max)" />
        <arg name="allied_wide_camera_gamma" value="$(arg allied_wide_camera_gamma)" />
        <arg name="allied_wide_camera_saturation" value="$(arg allied_wide_camera_saturation)" />
        <arg name="allied_wide_camera_hue" value="$(arg allied_wide_camera_hue)" />
        <arg name="allied_wide_camera_intensity_auto_precedence" value="$(arg allied_wide_camera_intensity_auto_precedence)" />
        <arg name="allied_wide_camera_auto_white_balance" value="$(arg allied_wide_camera_auto_white_balance)" />
        <arg name="allied_wide_camera_balance_ratio_selector" value="$(arg allied_wide_camera_balance_ratio_selector)" />
        <arg name="allied_wide_camera_balance_ratio" value="$(arg allied_wide_camera_balance_ratio)" />
        <arg name="allied_wide_camera_balance_white_auto_rate" value="$(arg allied_wide_camera_balance_white_auto_rate)" />
        <arg name="allied_wide_camera_balance_white_auto_tolerance" value="$(arg allied_wide_camera_balance_white_auto_tolerance)" />
        <arg name="allied_wide_camera_intensity_controller_region" value="$(arg allied_wide_camera_intensity_controller_region)" />
        <arg name="allied_wide_camera_intensity_controller_target" value="$(arg allied_wide_camera_intensity_controller_target)" />

        <arg name="allied_narrow_camera_exposure_time" value="$(arg allied_narrow_camera_exposure_time)" />
        <arg name="allied_narrow_camera_auto_exposure_time" value="$(arg allied_narrow_camera_auto_exposure_time)" />
        <arg name="allied_narrow_camera_auto_exposure_time_range_min" value="$(arg allied_narrow_camera_auto_exposure_time_range_min)" />
        <arg name="allied_narrow_camera_auto_exposure_time_range_max" value="$(arg allied_narrow_camera_auto_exposure_time_range_max)" />
        <arg name="allied_narrow_camera_gain" value="$(arg allied_narrow_camera_gain)" />
        <arg name="allied_narrow_camera_auto_gain" value="$(arg allied_narrow_camera_auto_gain)" />
        <arg name="allied_narrow_camera_auto_gain_range_min" value="$(arg allied_narrow_camera_auto_gain_range_min)" />
        <arg name="allied_narrow_camera_auto_gain_range_max" value="$(arg allied_narrow_camera_auto_gain_range_max)" />
        <arg name="allied_narrow_camera_gamma" value="$(arg allied_narrow_camera_gamma)" />
        <arg name="allied_narrow_camera_saturation" value="$(arg allied_narrow_camera_saturation)" />
        <arg name="allied_narrow_camera_hue" value="$(arg allied_narrow_camera_hue)" />
        <arg name="allied_narrow_camera_intensity_auto_precedence" value="$(arg allied_narrow_camera_intensity_auto_precedence)" />
        <arg name="allied_narrow_camera_auto_white_balance" value="$(arg allied_narrow_camera_auto_white_balance)" />
        <arg name="allied_narrow_camera_balance_ratio_selector" value="$(arg allied_narrow_camera_balance_ratio_selector)" />
        <arg name="allied_narrow_camera_balance_ratio" value="$(arg allied_narrow_camera_balance_ratio)" />
        <arg name="allied_narrow_camera_balance_white_auto_rate" value="$(arg allied_narrow_camera_balance_white_auto_rate)" />
        <arg name="allied_narrow_camera_balance_white_auto_tolerance" value="$(arg allied_narrow_camera_balance_white_auto_tolerance)" />
        <arg name="allied_narrow_camera_intensity_controller_region" value="$(arg allied_narrow_camera_intensity_controller_region)" />
        <arg name="allied_narrow_camera_intensity_controller_target" value="$(arg allied_narrow_camera_intensity_controller_target)" />
    </include>

    <!-- Start streaming the avaliable sensors. -->
    <include file="$(find l3cam_ros)/launch/stream_l3cam.launch" if="$(eval arg('stream'))">
        <arg name="rviz" value="$(arg rviz)" />
    </include>

    <!-- The nodes that don't have their sensor avaliable will automatically shut down -->
</launch>